# This is Git's per-user configuration file.
[user]
# Please adapt and uncomment the following lines:
	name = daryn
	email = daryn@hex50.com
[core]
	# editor = atom --wait
	editor = code --wait
[alias]
	graph = log --oneline --decorate --graph --all
	all = !git status && git add . && git commit && git push origin master
[commit]
    template = ~/.gitmessage
